@model University.Data.Applicant

@{
    ViewBag.Title = "GetApplicant";
}

<h2>Your Profile</h2>

<div>
    <dl class="dl-horizontal">
        <dt>
            Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ApplicantName)
        </dd>

        <dt>
            Address
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ApplicantAddress)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Phone)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Phone)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Email)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "EditApplicant", new { id = Model.Id }, new { @class="btn btn-primary btn-sm" })
    @Html.ActionLink("Delete Profile", "DeleteApplicant", new { id = Model.Id }, new { @class = "btn btn-danger btn-sm" })
</p>
